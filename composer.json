{
    "name": "petitglacon/category-treebuilder",
    "type": "typo3-cms-extension",
    "description": "Build and manage your category tree.",
    "authors": [],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^12.4",
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Petitglacon\\CategoryTreebuilder\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/category-treebuilder": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "category_treebuilder"
        }
    }
}